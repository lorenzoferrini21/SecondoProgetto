services:
  - type: web
    plan: free
    name: SecondoProgetto
    runtime: python
    buildCommand: "./build.sh"
    startCommand: "python -m gunicorn FerriniLorenzoPPM.asgi:application -k uvicorn.workers.UvicornWorker"
      envVars:
      - key: DATABASE_URL
        value: "https://github.com/railwayapp-templates/postgres-ssl/pkgs/container/postgres-ssl"
      - key: SECRET_KEY
        generateValue: true
      - key: WEB_CONCURRENCY
        value: 4